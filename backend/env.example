# Environment variables for Smart Course Recommendation System
# Copy this file to .env and update the values

# Project Information
PROJECT_NAME=Smart Course Recommendation System
VERSION=0.1.0
API_V1_STR=/api/v1

# Security
SECRET_KEY=your-secret-key-change-in-production-please-use-a-strong-key
ACCESS_TOKEN_EXPIRE_MINUTES=10080  # 7 days

# CORS Origins (comma-separated)
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:19006

# Allowed Hosts (comma-separated)
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# Database Configuration
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=course_recommendation
POSTGRES_PORT=5432

# Redis Configuration
REDIS_URL=redis://localhost:6379

# AI/ML Configuration
MODEL_PATH=./ai-ml/models
RECOMMENDATION_BATCH_SIZE=100
RECOMMENDATION_CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO
